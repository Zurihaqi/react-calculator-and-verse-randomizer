{
    "functions": {
      "api/server.js": {
        "memory": 1024,
        "includeFiles": "db.json"
      }
    },
    "rewrites": [
      {
        "source": "api/(.*)",
        "destination": "server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }